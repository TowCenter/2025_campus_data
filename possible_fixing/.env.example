# .env.example - Copy this to .env and fill in your values

# Server Configuration
PORT=3001
NODE_ENV=production

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/university_tracker
# Example for Supabase: postgresql://postgres:[password]@[host]:5432/postgres
# Example for Railway: postgresql://postgres:[password]@[host]:[port]/railway

# Redis Cache Configuration
REDIS_URL=redis://localhost:6379
# Example for Upstash: redis://:[password]@[host]:[port]
# Example for Railway: redis://default:[password]@[host]:[port]

# Security
JWT_SECRET=your-super-secret-jwt-key-here
API_KEY=your-api-key-for-admin-endpoints

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173,https://yourdomain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Cache Configuration
CACHE_TTL_SCHOOLS=3600
CACHE_TTL_RESPONSES=300
CACHE_TTL_MONTHS=3600

# AWS S3 (if still needed for backups)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-2
S3_BUCKET=2025-campus-data

# Logging
LOG_LEVEL=info